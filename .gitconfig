[push]
	default = simple
[alias]
	ac = ! $HOME/bin/git/ac
	amend = ! $HOME/bin/git/amend
  commands = ! $HOME/bin/git/commands
	dt = difftool
	isrepo = ! $HOME/bin/git/isrepo
	provider = ! $HOME/bin/git/provider
	rename-tag = ! $HOME/bin/git/rename-tag
	root = ! $HOME/bin/git/root
	set-author = ! $HOME/bin/git/set-author
  b = ! $HOME/bin/git/b
  base = ! $HOME/bin/git/base 
  pr = ! $HOME/bin/git/pr 
  prs = ! $HOME/bin/git/prs 
  rr = ! $HOME/bin/git/rr 
  squash = ! $HOME/bin/git/squash
  umr = ! $HOME/bin/git/umr
  update = ! $HOME/bin/git/update
[checkout]
    defaultRemote = origin
[diff]
	tool = vimdiff
[difftool]
    cmd = vim -f -c MergetoolStart "$MERGED" "$BASE" "$LOCAL" "$REMOTE"
	prompt = false
[merge]
	tool = diffconflicts
	conflictstyle = diff3
[mergetool]
	keepBackup = false
	prompt = false
[mergetool "vim_mergetool"]
    cmd = vim -f -c MergetoolStart "$MERGED" "$BASE" "$LOCAL" "$REMOTE"
    trustExitCode=true
[credential]
	helper = store
[core]
	ignorecase = false
	hooksPath =
[user]
	name = Chris Paterson
	email = paterson@adobe.com
[pull]
	rebase = false
[credential "https://github.com"]
	helper = 
	helper = !/usr/local/bin/gh auth git-credential
[credential "https://git.corp.adobe.com"]
	helper = 
	helper = !/usr/local/bin/gh auth git-credential
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[mergetool "diffconflicts"]
	cmd = vim -c DiffConflicts \"$MERGED\" \"$BASE\" \"$LOCAL\" \"$REMOTE\"
	trustExitCode = true

[push]
	default = simple
[alias]
	ac = ! $HOME/bin/git/ac
	amend = ! $HOME/bin/git/amend
	dt = difftool
	isrepo = ! $HOME/bin/git/isrepo
	provider = ! $HOME/bin/git/provider
	rename-tag = ! $HOME/bin/git/rename-tag
	root = ! $HOME/bin/git/root
	set-author = ! $HOME/bin/git/set-author
	start = ! $HOME/bin/git/start
  b = ! $HOME/bin/git/b
  base = ! $HOME/bin/git/base 
  pr = ! $HOME/bin/git/pr 
  squash = ! $HOME/bin/git/squash
  update = ! $HOME/bin/git/update
[checkout]
    defaultRemote = origin
[diff]
	tool = vimdiff
[difftool]
    cmd = vim -f -c MergetoolStart "$MERGED" "$BASE" "$LOCAL" "$REMOTE"
	prompt = false
[merge]
	tool = vim_mergetool
	conflictstyle = diff3
[mergetool]
    keepBackup=false
	prompt = false
[mergetool "vim_mergetool"]
    cmd = vim -f -c MergetoolStart "$MERGED" "$BASE" "$LOCAL" "$REMOTE"
    trustExitCode=true
[credential]
	helper = store
[core]
	ignorecase = false
	hooksPath =
[user]
	name = Chris Paterson
[pull]
	rebase = false
[credential "https://github.com"]
	helper = 
	helper = !/usr/local/bin/gh auth git-credential
[credential "https://git.corp.adobe.com"]
	helper = 
	helper = !/usr/local/bin/gh auth git-credential
